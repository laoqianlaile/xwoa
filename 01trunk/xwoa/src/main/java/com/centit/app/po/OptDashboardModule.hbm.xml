<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.centit.app.po.OptDashboardModule" table="OPT_DASHBOARD_MODULE"  >
		<id name="id" type="java.lang.Long">
			<column name="ID"/>
		</id>
	
		<property name="moduleName" type="java.lang.String">
			<column name="MODULENAME"/>
		</property>
		<property name="displayUrl" type="java.lang.String">
			<column name="DISPLAYURL"  />
		</property>
		<property name="linkUrl" type="java.lang.String">
			<column name="LINKURL"/>
		</property>
		<property name="moduleCode" type="java.lang.String">
			<column name="MODULECODE"/>
		</property>
		<property name="isUsed" type="java.lang.String">
			<column name="ISUSED"/>
		</property>
	</class>
</hibernate-mapping>