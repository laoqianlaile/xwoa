<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.centit.sys.po.FUserForgetPassword" table="F_USERFORGETPASSWORD">
		<id name="djId" type="java.lang.String">
			<column name="djId" length="50" />
			<generator class="assigned" />
		</id>
		<property name="usercode" type="java.lang.String">
			<column name="usercode" length="8" />
		</property>
		<property name="telephone" type="java.lang.String">
			<column name="telephone" length="64" />
		</property>
		<property name="ip" type="java.lang.String">
			<column name="ip" length="16" />
		</property>
		<property name="state" type="java.lang.String">
			<column name="state" precision="1" />
		</property>
		<property name="forgettime" type="java.util.Date">
			<column name="forgettime" length="7" />
		</property>
		<property name="validatetime" type="java.util.Date">
			<column name="validatetime" length="7" />
		</property>
		<property name="validatenum" type="java.lang.String">
			<column name="validatenum" length="16" />
		</property>
	</class>
</hibernate-mapping>
