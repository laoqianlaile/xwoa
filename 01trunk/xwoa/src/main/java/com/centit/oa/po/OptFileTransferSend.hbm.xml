<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.centit.oa.po">
	<class name="OptFileTransferSend" table="OPT_FILETRANSFER_SEND" >
		<id name="id" type="java.lang.String">
			<column name="ID"/>
			<generator class="assigned" />
		</id>
		<property name="receiverName" type="java.lang.String">
			<column name="RECEIVERNAME"/>
		</property>
		<property name="senderCode" type="java.lang.String">
			<column name="SENDERCODE" length="8" />
		</property>
		<property name="scopeType" type="java.lang.String">
			<column name="SCOPETYPE" length="5" />
		</property>
		<property name="sendType" type="java.lang.Integer">
			<column name="SENDTYPE"/>
		</property>
		<property name="subject" type="java.lang.String">
			<column name="SUBJECT"  />
		</property>
		<property name="remark" type="java.lang.String">
			<column name="REMARK"  />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATETIME"  />
		</property>
	</class>
</hibernate-mapping>